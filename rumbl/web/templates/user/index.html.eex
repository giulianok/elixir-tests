<h1>Listing Users</h1>

<table class="table">
  <div class="text-right">
    <%= link "Create User", to: user_path(@conn, :new), class: "btn btn-primary" %>
  </div>
  <%= for user <- @users do %>
    <tr>
      <td><%= render "user.html", user: user %></td>
      <td><%= link "View", to: user_path(@conn, :show, user.id) %></td>
    </tr>
  <% end %>
</table>
